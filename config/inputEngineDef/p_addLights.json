{
	"title": "add multiple lights",
	"type": "object",
	"properties": {
		"voice_interpretation": {
			"type": "object",
			"attraction": "weak",
			"properties": {
				"event": {
					"type":"string",
					"enum": ["RecoComplete"]
				},
				"semantic": {
					"type": "object",
					"properties": {
						"addLight": {
							"type": "object",
							"properties": {
								"lights": {
									"type": "string",
									"not": 
									{
										"enum": ["undefined"]
									}
								}
								
							},
							"required": ["lights"]
						}
					},
					"required": ["addLight"]
				},
				"start": {
					"type": "number"
				},
				"end": {
					"type": "number"
				}

			},
			"required": ["event", "semantic", "start", "end"]
		},

		"touch_interpretation": {
			"type": "array",
			"minItems": 1,
			"maxItems": 9,
			"maintain": 1500,
			"items": {
				"properties" : {
					"x": {
						"type": "number"
					},
					"y": {
						"type": "number"
					},
					"timestamp": {
						"type": "number"
					}
				},
				"required": ["x","y", "timestamp"]
			}
		},

		"context_interpretation": {
			"type": "object",
			"attraction": "weak",
			"properties": {
				"currentViewName": {
					"type": "string",
					"enum": ["main", "welcome"]
				}
			},
			"required": ["currentViewName"]
		}
	},
	"required": ["voice_interpretation", "touch_interpretation", "context_interpretation"],


	"output": {
		"event": "addLights",
        "target": "Application",
		"data": {
			"scope": {
				"lightPosition" : "touch_interpretation"
			}
		}
	},

	"expired": 4000
}